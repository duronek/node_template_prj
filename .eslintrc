{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    //"standard",
    //"eslint:recommended"
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2015
  },
  "rules": {
    "no-plusplus": "off",
    "no-console": "off",
    "import/extensions": "off",
    "max-len": "off"
  }
}
